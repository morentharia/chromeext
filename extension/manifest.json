{
  "name": "razdva36 helper",
  "description": "razdva36 helper",
  "version": "1.0",
  "permissions": [
    "storage",
    "declarativeContent",
    "tabs",
    "http://localhost/",
    "background",
    "activeTab"
  ],
  "background": {
    "scripts": [
        "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_start",
      "all_frames": true,
      "js": [
        "content.js"
      ]
    }
  ],
  "page_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "128x128.png"
  },
  "manifest_version": 2
}
